{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Electron: Main",
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
        "program": "${workspaceFolder}/main.js",
        "cwd": "${workspaceFolder}",
        "env": {
          "ELECTRON_DISABLE_SECURITY_WARNINGS": "true"
        },
        "console": "integratedTerminal"
      }
    ]
  }
  